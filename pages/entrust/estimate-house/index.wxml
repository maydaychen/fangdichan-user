<!--pages/entrust/estimate-house/index.wxml-->
<view class="estimate-house" hidden="true">
  <view class="top">
    <text class="title">我的房子值多少钱</text>
    <text class="sub-title">累计超过10000000次估计请求</text>
  </view>

  <view class="address">
    <view class="select-group">
      <view class="left">
        <text class="iconfont icon-house-estimate icon"></text>
        <text class="left">华达华府</text>
      </view>
      <view class="right">
        <text class="iconfont icon-you select-icon"></text>
      </view>
    </view>
    <view class="select-group">
      <view class="left">
        <text class="iconfont icon-house-solid icon"></text>
        <text class="left">9号楼1单元904</text>
      </view>
      <view class="right">
        <text class="iconfont icon-you select-icon"></text>
      </view>
    </view>
  </view>

  <button class="estimate-btn">开始估价</button>

  <view class="no-clear">不清楚门牌号</view>

  <view class="info-icon">
    <image src="/image/info-icon.png" />
  </view>

  <view class="input-info-notice">请输入信息股价</view>
</view>

<view class="input-info">
  <view class="part">
    <view class="select-group">
      <text class="left">小区名称</text>
      <text class="iconfont icon-you select-icon"></text>
    </view>
    <view class="select-group">
      <text class="left">房屋户型</text>
      <text class="iconfont icon-you select-icon"></text>
    </view>
  </view>
  <view class="part">
    <view class="select-group">
      <text class="left">单元</text>
      <text>真实价格 万元</text>
    </view>
    <view class="select-group">
      <text class="left">产权面积</text>
      <text class="iconfont icon-you select-icon"></text>
    </view>
    <view class="select-group">
      <text class="left">房屋类型</text>
      <text class="iconfont icon-you select-icon"></text>
    </view>
    <view class="select-group">
      <text class="left">唯一住房</text>
      <text class="iconfont icon-you select-icon"></text>
    </view>
  </view>
  <view class="part">
    <view class="select-group">
      <text class="left">朝向</text>
      <text class="iconfont icon-you select-icon"></text>
    </view>
    <view class="select-group">
      <text class="left">装修</text>
      <text class="iconfont icon-you select-icon"></text>
    </view>
    <view class="select-group">
      <text class="left">电梯</text>
      <text class="iconfont icon-you select-icon"></text>
    </view>
    <view class="select-group">
      <text class="left">楼层</text>
      <text class="iconfont icon-you select-icon"></text>
    </view>
  </view>
  <view class="part">
    <view class="select-group">
      <text class="left">产权类型</text>
      <text class="iconfont icon-you select-icon"></text>
    </view>
    <view class="select-group">
      <text class="left">房屋年限</text>
      <text class="iconfont icon-you select-icon"></text>
    </view>
  </view>

  <button class="estimate-btn">开始估价</button>
</view>
