/* pages/twohand/index.wxss */
/* pages/house/twohand/index.wxss */

page {
  background: #fff;
}

@font-face {
  font-family: "iconfont";
  src: url('iconfont.eot?t=1541061234374');
  /* IE9*/
  src: url('iconfont.eot?t=1541061234374#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('iconfont.ttf?t=1541061234374') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+*/ url('iconfont.svg?t=1541061234374#iconfont') format('svg');
  /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-add:before {
  content: "\e601";
}

.icon-cx:before {
  content: "\e600";
}

.icon-time:before {
  content: "\e604";
}

.list {
  background: #fff;
  padding: 20rpx;
}

.list>image {
  width: 115%;
  height: 5rpx;
  margin-left: -10%;
}

.list>view {
  overflow: hidden;
}

.list>view .left:first-child {
  width: 190rpx;
  height: 190rpx;
}

.sale {
  padding: 20rpx 0rpx;
}

.sale text {
  font-size: 28rpx;
  padding: 5rpx 10rpx;
  border-radius: 5rpx;
  margin-right: 10rpx;
}

.sale text:first-child {
  color: #e40c01;
  background: #fff8e0;
}

.sale text:nth-child(2), .sale text:last-child {
  color: #1c87d8;
  background: rgba(191, 220, 242, 0.5);
}

.sale text:nth-child(3) {
  color: #5cb66a;
  background: rgba(202, 240, 158, 0.5);
}

.left {
  font-size: 28rpx;
  margin-right: 20rpx;
  width: 67%;
}

.left>text {
  color: #8a8a8a;
}

.list>view .right {
  width: 60rpx;
  height: 45rpx;
  margin-left: 8%;
}

.list>view:last-child {
  border-bottom: 1px solid #eee;
  padding-top: 15rpx;
}

.list>view:last-child text {
  color: #ddd;
}

.list>view:last-child .d {
  color: #999;
  font-size: 26rpx;
  margin-left: 10rpx;
}

.list .bian {
  color: #666;
  background: #eee;
  padding: 10rpx;
  line-height: 2;
  border-radius: 0px 4px 4px 0px;
}

.bians {
  margin-left: 15%;
}

.top {
  padding: 20rpx;
}

.top>text {
  font-size: 60rpx;
  color: #999;
  vertical-align: middle;
  margin-left: 5%;
}

input {
  display: inline-block;
  width: 500rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}

.search {
  padding: 15rpx 30rpx;
  border-radius: 50rpx;
  display: inline-block;
  background: #f5f5f5;
}

.search text {
  font-size: 35rpx;
  vertical-align: middle;
  color: #777;
  font-weight: bold;
}

.list .left .down {
  color: #fff;
  background: #1e87d9;
  border-radius: 4px 0px 0px 4px;
}

.title {
  height: 70rpx;
  line-height: 35rpx;
  font-size: var(--font-size-large);
  color: var(--color-certification-title);
}
